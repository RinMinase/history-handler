{
  "name": "history-handler",
  "description": "Handles your undo and redo, customizable to your liking",
  "version": "1.0.1",
  "license": "MIT",
  "author": "Rin Minase",
  "main": "index.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RinMinase/history-handler.git"
  },
  "keywords": [
    "history",
    "undo",
    "redo",
    "history-handler"
  ],
  "scripts": {
    "build": "npm run build:pre && npm run build:pretty && npm run build:min",
    "build:pre": "rm -rf dist/ && mkdir dist",
    "build:pretty": "uglifyjs index.js -b -o dist/history-handler.js",
    "build:min": "uglifyjs index.js -c -m -o dist/history-handler.min.js",
    "test": "nyc --reporter=lcov mocha",
    "zip": "bestzip dist.zip dist/ && npm run zip:post",
    "zip:post": "node -e \"require('fs').rename('dist.zip', 'history-handler-'+process.env.npm_package_version+'-dist.zip', ()=>{})\"",
    "release": "release-it --ci --no-increment --verbose || true"
  },
  "dependencies": {},
  "devDependencies": {
    "bestzip": "^2.1.5",
    "chai": "^4.2.0",
    "codecov": "^3.6.5",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "release-it": "^13.5.4",
    "uglify-es": "^3.3.9"
  },
  "release-it": {
    "git": {
      "addUntrackedFiles": false,
      "commit": false,
      "tag": false,
      "push": false,
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true,
      "releaseName": "${version}",
      "assets": "history-handler-*-dist.js"
    },
    "npm": {
      "publish": true,
      "publishConfig": {
        "access": "public"
      }
    }
  }
}
