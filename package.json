{
  "name": "history-handler",
  "description": "Handles your undo and redo, customizable to your liking",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Rin Minase",
  "main": "index.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RinMinase/history-handler.git"
  },
  "keywords": [
    "history",
    "undo",
    "redo",
    "history-handler"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:compile": "babel index.js --out-file dist/history-handler.js",
    "build:minify": "terser dist/history-handler.js -c -m --comments false -o dist/history-handler.min.js",
    "release": "release-it --ci --no-increment --verbose || true",
    "test": "run-s test:*",
    "test:clean": "rm -rf .nyc_output/ coverage/",
    "test:execute": "nyc --reporter=lcov --reporter=text-summary mocha",
    "test:post": "rm -rf .nyc_output/",
    "zip": "run-s zip:*",
    "zip:pre": "bestzip dist.zip dist/",
    "zip:post": "node -e \"require('fs').rename('dist.zip', 'history-handler-'+process.env.npm_package_version+'-dist.zip', ()=>{})\""
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "bestzip": "^2.1.5",
    "chai": "^4.2.0",
    "codecov": "^3.6.5",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "release-it": "^13.5.5",
    "terser": "^4.6.11"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "release-it": {
    "git": {
      "addUntrackedFiles": false,
      "commit": false,
      "tag": false,
      "push": false,
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true,
      "releaseName": "${version}",
      "assets": "history-handler-*-dist.js"
    },
    "npm": {
      "publish": true,
      "publishConfig": {
        "access": "public"
      }
    }
  }
}
